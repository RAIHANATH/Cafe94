        </Button>
        <Button fx:id="viewStaffAccount" layoutX="840.0" layoutY="435.0" mnemonicParsing="false" prefHeight="17.0"
                prefWidth="163.0" text="Staff account">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Button fx:id="staffLogOut" layoutX="839.0" layoutY="520.0" mnemonicParsing="false" onAction="#switchToLogIn"
                prefHeight="17.0" prefWidth="163.0" text="Log out">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Label layoutX="819.0" layoutY="14.0" prefHeight="82.0" prefWidth="207.0" text="Controls:" textFill="WHITE">
            <font>
                <Font name="Times New Roman Italic" size="52.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
