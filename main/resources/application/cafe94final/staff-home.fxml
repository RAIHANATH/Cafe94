<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="application.cafe94final.StaffHomeController">
    <children>
        <ImageView fitHeight="645.0" fitWidth="1151.0" layoutX="-14.0" layoutY="-17.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@cat.gif"/>
            </image>
        </ImageView>
        <Label layoutX="24.0" layoutY="18.0" prefHeight="71.0" prefWidth="84.0" text="94" textFill="WHITE">
            <font>
                <Font name="Futura Medium" size="61.0"/>
            </font>
        </Label>
        <Label layoutX="34.0" layoutY="114.0" prefHeight="69.0" prefWidth="487.0" text="Have a good day!"
               textFill="WHITE">
            <font>
                <Font name="Times New Roman" size="61.0"/>
            </font>
        </Label>
        <Separator layoutX="20.0" layoutY="185.0" prefHeight="24.0" prefWidth="663.0"/>
        <Label layoutX="350.0" layoutY="195.0" prefHeight="45.0" prefWidth="704.0" text="SELL BEANS!" textFill="WHITE">
            <font>
                <Font name="Times New Roman" size="34.0"/>
            </font>
        </Label>
        <Button fx:id="takeOrder" layoutX="839.0" layoutY="97.0" mnemonicParsing="false" onAction="#switchToTakeOrder"
                prefHeight="17.0" prefWidth="163.0" text="Take order">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Button fx:id="editMenu" layoutX="839.0" layoutY="266.0" mnemonicParsing="false" onAction="#switchToEditMenu"
                prefHeight="35.0" prefWidth="164.0" text="Edit menu">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Button fx:id="viewStaffOrders" layoutX="839.0" layoutY="179.0" mnemonicParsing="false"
                onAction="#switchToViewOrder" prefHeight="17.0" prefWidth="163.0" text="View orders">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Button fx:id="viewStaffBooking" layoutX="840.0" layoutY="350.0" mnemonicParsing="false" prefHeight="17.0"
                prefWidth="163.0" text="View bookings">
            <font>
                <Font size="18.0"/>
